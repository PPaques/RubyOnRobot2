.page-header
  = link_to task_lists_path, :class => 'btn' do
    %i.icon-list-alt
    Back
  = link_to edit_task_list_path(@task_list), :class => 'btn btn-primary' do
    %i.icon-pencil.icon-white
    Edit
  %h1 Listing task_lists

%dl
  %dt Task:
  %dd= @task_list.task_id
  %dt Position:
  %dd= @task_list.position
